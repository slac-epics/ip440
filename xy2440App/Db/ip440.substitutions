	{ "$(CARD):port$(PORT):in$(CH)"	, 3,	4,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 3,	5,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 3,	6,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
