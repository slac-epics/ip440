#
# ZNAM - Zero value name
# ONAM - One value name
# ZSV  - Zero severity state
# OSV  - Zero severity state
#
file ip440.template
{
	pattern
	{ RECORDNAME              		, PORT,	CH,	DESC           		  , SCAN        , ZNAM  , ONAM,	ZSV,		OSV			}
	{ "$(CARD):port$(PORT):in$(CH)"	, 0,	0,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"	}
	{ "$(CARD):port$(PORT):in$(CH)"	, 0,	1,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"	}
	{ "$(CARD):port$(PORT):in$(CH)"	, 0,	2,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 0,	3,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
