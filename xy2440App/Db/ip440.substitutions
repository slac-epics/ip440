	{ "$(CARD):port$(PORT):in$(CH)"	, 0,	5,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 0,	6,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 0,	7,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }

	{ "$(CARD):port$(PORT):in$(CH)"	, 1,	0,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 1,	1,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 1,	2,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 1,	3,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 1,	4,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 1,	5,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 1,	6,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 1,	7,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }

	{ "$(CARD):port$(PORT):in$(CH)"	, 2,	0,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 2,	1,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 2,	2,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 2,	3,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 2,	4,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 2,	5,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 2,	6,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }
	{ "$(CARD):port$(PORT):in$(CH)"	, 2,	7,	"Port $(PORT) In$(CH)", ".1 second" , "Off" , "On",	"NO_ALARM",	"NO_ALARM"  }

